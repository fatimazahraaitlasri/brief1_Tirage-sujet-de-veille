#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/youcode/Desktop/les briefs de 2ème année 2022/brief 1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/youcode/Desktop/les briefs de 2ème année 2022/brief 1/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
